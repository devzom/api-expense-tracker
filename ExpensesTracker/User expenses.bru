meta {
  name: User expenses
  type: http
  seq: 4
}

get {
  url: http://localhost:3000/expenses/users/{{userId}}?category=food&sortOrder=desc
  body: json
  auth: inherit
}

params:query {
  category: food
  sortOrder: desc
  ~minAmount: 10
  ~sortBy: amount
  ~type: expense
  ~maxAmount: 333
  ~page: 1
  ~limit: 20
}
